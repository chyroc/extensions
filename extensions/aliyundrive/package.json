{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aliyundrive",
  "title": "AliyunDrive",
  "description": "Aliyundrive Search Files Extension",
  "icon": "aliyundrive.png",
  "author": "chyroc",
  "license": "MIT",
  "commands": [
    {
      "name": "search-files",
      "title": "AliyunDrive Search Files",
      "description": "search my files",
      "mode": "view"
    },
    {
      "name": "logout",
      "title": "AliyunDrive Logout",
      "description": "clean login auth cache",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@chyroc/aliyundrive": "^0.8.0",
    "@raycast/api": "^1.28.0",
    "qrcode": "^1.5.0",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
